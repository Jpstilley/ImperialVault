@model IEnumerable<Game>

<h2>Game Details</h2>

<table class="table">
    <tr>
        <th>Campaign Name</th>
    </tr>


    @foreach (var game in Model)
     {
        <tr>
            <td><a href=/Game/ViewGame/@game.CampaignID>@game.CampaignName</a></td>
        </tr>
     }
    </table>

<div style="display: block">
    <a href="/Game/CreateGame/">Create A New Game</a>
</div>
